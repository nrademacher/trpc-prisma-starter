{
  "name": "trpc-prisma-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:1-migrate": "prisma migrate deploy",
    "build:2-build": "next build",
    "build": "run-s build:*",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "yarn prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dev": "next dev",
    "dx": "run-s db-up db-migrate-dev dev",
    "start": "next start",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|tsx|js|css|json)\" --write --list-different --config prettier.config.js",
    "lint": "eslint src",
    "lint-fix": "yarn lint --fix",
    "test-dev": "start-server-and-test dev 3000 test",
    "test-start": "start-server-and-test start 3000 test",
    "test": "run-s test:*",
    "test:e2e": "playwright test",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "generate-schemas": "prisma generate && prisma format",
    "postinstall": "yarn generate-schemas",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./{*,src/**/*}.{js,ts,jsx,tsx}": [
      "yarn format",
      "yarn lint-fix"
    ],
    "./prisma/schema.prisma": [
      "prisma format"
    ]
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.1",
    "@prisma/client": "^3.15.2",
    "@trpc/client": "^9.25.2",
    "@trpc/next": "^9.25.2",
    "@trpc/react": "^9.25.2",
    "@trpc/server": "^9.25.2",
    "bcrypt": "^5.0.1",
    "next": "^12.1.0",
    "next-auth": "^4.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.32.1",
    "react-query": "^3.39.1",
    "start-server-and-test": "^1.12.0",
    "superjson": "^1.8.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@next-auth/prisma-adapter": "^1.0.3",
    "@playwright/test": "^1.22.2",
    "@tailwindcss/forms": "^0.5.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.13",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.30.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=8",
    "lint-staged": ">=13.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "prisma": "^3.15.2",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "typescript": "4.7.3",
    "zod-prisma": "^0.5.4"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
